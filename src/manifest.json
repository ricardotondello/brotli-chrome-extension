{
    "name": "Brotli Decompress Plugin",
    "description": "This extension will decompress brotli hashes",
    "version": "1.0",
    "icons": {
        "16": "icon.png"
    },
    "manifest_version": 3,
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*", "<all_urls>"],
        "js": ["bundle.js"]
      }
    ]
}